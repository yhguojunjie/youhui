<script type="text/javascript">
	$(function(){

		$('.classic ul dd').css("height",$('.classic ul dd').width()*55/32+"px");
		$('.classic ul dd img').css({"width":$('.classic ul dd').width()+"px","height":$('.classic ul dd').width()*55/32+"px"});
		$('.classic ul dd .imgs').css("height",$('.classic ul dd').width()*55/32+"px");

	});

	function go_next(obj){
		var $obj = $(obj).parent().parent().find('.imgs');
		$obj.animate({marginLeft:'-300px'},300,function(){
			$obj.find('.img:first').appendTo($obj);
			$obj.css("margin-left","0px");
		});
	}
	function go_prev(obj){
		var $obj = $(obj).parent().parent().find('.imgs');
		$obj.css("margin-left","-300px").find('.img:last').prependTo($obj);
		$obj.animate({marginLeft:'0px'},300);
	}
</script>



